package com.shinowit.messageboard.model;

import java.sql.Date;

public class UserInfo {
	private int user_id;
	
	private int sex_type_code;
	
	private String login_name;
	
	private String login_pass;
	
	private String pass_question;
	
	private String pass_ques_answer;
	
	private Date birthday;
	
	private String nick_name;
	
	private String ques_email_addr;
	
	private String pic;
	
	public int getUser_id() {
		return user_id;
	}
	public void setUser_id(int user_id) {
		this.user_id = user_id;
	}
	public int getSex_type_code() {
		return sex_type_code;
	}
	public void setSex_type_code(int sex_type_code) {
		this.sex_type_code = sex_type_code;
	}
	public String getLogin_name() {
		return login_name;
	}
	public void setLogin_name(String login_name) {
		this.login_name = login_name;
	}
	public String getLogin_pass() {
		return login_pass;
	}
	public void setLogin_pass(String login_pass) {
		this.login_pass = login_pass;
	}
	public String getPass_question() {
		return pass_question;
	}
	public void setPass_question(String pass_question) {
		this.pass_question = pass_question;
	}
	public String getPass_ques_answer() {
		return pass_ques_answer;
	}
	public void setPass_ques_answer(String pass_ques_answer) {
		this.pass_ques_answer = pass_ques_answer;
	}
	public Date getBirthday() {
		return birthday;
	}
	public void setBirthday(Date birthday) {
		this.birthday = birthday;
	}
	public String getNick_name() {
		return nick_name;
	}
	public void setNick_name(String nick_name) {
		this.nick_name = nick_name;
	}
	public String getQues_email_addr() {
		return ques_email_addr;
	}
	public void setQues_email_addr(String ques_email_addr) {
		this.ques_email_addr = ques_email_addr;
	}
	public String getPic() {
		return pic;
	}
	public void setPic(String pic) {
		this.pic = pic;
	}

}